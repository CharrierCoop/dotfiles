[core]
    excludesfile = /home/coopengo/.gitignore_global
[alias]
    co = checkout
    ci = commit --verbose
    st = status
    br = branch
    d = diff
    dc = diff --cached
    #l = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    l = log --color --graph --full-history --all --pretty=format:'%Cred%h%Creset -%C(cyan)%d%Creset %s %Cgreen(%cd) %C(bold blue)<%an>%Creset%n' --abbrev-commit --date iso
    type = cat-file -t
    dump = cat-file -p
    phomxx = push origin master
    plom = pull origin master
[color]
	ui = true
[format]
	pretty = oneline
[user]
	name = Antoine Roulin
	email = antoine.roulin@coopengo.com
